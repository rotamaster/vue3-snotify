{
  "name": "vue3-snotify",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build-lib": "vue-tsc --noEmit && vite build -c vite.lib.config.ts",
    "bl": "npm run build-lib",
    "serve": "vite preview"
  },
  "dependencies": {
    "mitt": "^3.0.1",
    "vue": "^3.4.30"
  },
  "devDependencies": {
    "@babel/types": "^7.24.7",
    "@types/node": "^20.14.8",
    "@vitejs/plugin-vue": "^5.0.5",
    "sass": "^1.77.4",
    "typescript": "^5.4.5",
    "vite": "^5.3.2",
    "vite-plugin-dts": "^3.9.1",
    "vue-tsc": "^2.0.22"
  },
  "main": "./dist/index.es.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js"
    },
    "./style": "./dist/index.css",
    "./theme/material": "./dist/material.css",
    "./theme/simple": "./dist/simple.css",
    "./theme/dark": "./dist/dark.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rotamaster/vue3-snotify"
  }
}
